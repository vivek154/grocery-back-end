{
    "name": "node-typescript-api",
    "version": "0.0.1",
    "description": "Node-Express Starter Kit with TypeScript",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev -r dotenv/config src/index.ts",
        "build": "tsc",
        "start": "node -r dotenv/config build/index.js",
        "deploy-prod": "npm install && npm run build && npm start",
        "lint": "eslint . --ext .js,.ts",
        "lint-fix": "eslint . --ext .js,.ts --fix",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/assimilate-technologies/node-typescript-api.git"
    },
    "keywords": [
        "NodeJS",
        "TypeScript",
        "Express",
        "JWT"
    ],
    "author": "Assimilate Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/assimilate-technologies/node-typescript-api/issues"
    },
    "homepage": "https://github.com/assimilate-technologies/node-typescript-api#readme",
    "devDependencies": {
        "@types/cors": "2.8.12",
        "@types/express": "4.17.13",
        "@types/jsonwebtoken": "8.5.8",
        "@types/node": "18.0.0",
        "@types/node-cron": "^3.0.2",
        "@types/otp-generator": "^4.0.0",
        "@types/passport": "1.0.9",
        "@types/passport-jwt": "3.0.6",
        "@typescript-eslint/eslint-plugin": "5.13.0",
        "@typescript-eslint/parser": "5.0.0",
        "dotenv": "16.0.1",
        "eslint": "8.2.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-airbnb-typescript": "17.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.25.2",
        "husky": "8.0.0",
        "prettier": "2.7.1",
        "ts-node": "10.8.1",
        "ts-node-dev": "2.0.0",
        "typescript": "4.7.4"
    },
    "dependencies": {
        "cors": "2.8.5",
        "express": "4.18.1",
        "jsonwebtoken": "8.5.1",
        "mysql2": "2.3.3",
        "node-cron": "3.0.1",
        "otp-generator": "4.0.0",
        "passport": "0.6.0",
        "passport-jwt": "4.0.0",
        "sequelize": "6.21.1",
        "sequelize-typescript": "2.1.3",
        "twilio": "3.78.0",
        "pg": "8.8.0"
    }
}
